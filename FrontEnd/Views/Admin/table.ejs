<table class="table">
	<thead>
	    <tr>
	     	<th scope="col">ID</th>
		    <th scope="col">Nombre</th>
		    <th scope="col">Apellidos</th>
		    <th scope="col">Correo</th>
		    <th scope="col">Modificar</th>
		    <th scope="col">Eliminar</th>
	    </tr>
	</thead>
	<tbody>
		<% for(var i = 0; i < data.length; i++ ){ %>
		    <tr>
		      	<th scope="row"> <%= data[i].id %> </th>
		      	<td><%= data[i].name %></td>
		      	<td><%= data[i].last_name %></td>
		      	<td><%= data[i].email %></td>
		      	<td><a href="/admin/modificar/user/<%= data[i].id %>" class="btn btn-info">Modificar</a></td>
		      	<td><a href="/admin/eliminar/user/<%= data[i].id %>" class="btn btn-danger">Eliminar</a></td>
		    </tr>
	    <% } %>
	</tbody>
</table>